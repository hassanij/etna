html,body {
  height: 100%;
  margin: 0px;
  padding: 0px;
  background: #34383c;
  overflow: hidden;
}

h1 {
  font-family: "Lucida Grande", "Lucida Sans Unicode", Arial, sans-serif;
  font-size: 5.8em;
  text-align: center;
  color: #123958;
}

#Persistent{
  background: url("../Images/home_tick.png") no-repeat 0 0;
  background-size: 105px 52px;
  padding-left: 22px;
	position: fixed;
	left: -12px;
	top: 0;
	width: 110px;
	height: 50px;
	cursor: pointer;
}

#map {
  width: 500px;
  height: 100%;
  margin: auto;
}

.row {
 padding-bottom: 2%;
 padding-top: 4%;
}

.button {
  width: 30%;
  margin-right: auto;
  margin-left: auto;
}

.mybtn{
  width: 100%;
}

.container-full {
  margin: 0 auto;
  width: 100%;
  min-height: 100%;
  background: #34383c;
  background-size: cover;
  color: #eee;
  overflow: hidden;
}

#content{
  width: 40%;
  height: 10%;
  min-width: 400px;
  min-height: 200px;
  margin: auto;
  margin-top: 20%;
  padding-bottom: 2%;
  border-radius: 14px;
  border-width: 2px;
  background-color: rgba(150, 150, 150, 0.7);
  font-size: 30px;
}

.panel {
    margin-top: -20%;
}

.panel-body {
  height: inherit;
  width: inherit;
}

.panel-success {
  width: 100%;
  height: 100%;
}

.headquizz {
  color: #FFFFFF;
  opacity: 0.4;
  width: 40%;
  height: 25%;
  min-width: 100px;
  min-height: 50px;
  margin: auto;
  margin-top: 5%;
  padding-bottom: 2%;
  background: black;
  color: white;
  border-radius: 16px;
  border-width: 2px;
  -moz-border-radius:10px;
  -webkit-border-radius:10px;
}

.modal-header {
  background-color: #28B62C;
  font-size: 0.4em;
  border-radius: 4px;
}

.modal-header h1 {
  color: white;
}

.quest-block {
  padding: 4px;
  border: 4px solid #e0e0e0;
}

.quest-content {
  padding: 3px;
  background-color: #000000;
}

#sortable {
    list-style-type: none;
    margin: 0;
    padding: 0;
    width: 60%;
}

#sortable li {
    margin: 0 3px 3px 3px;
    padding-left: 1.5em;
    font-size: 0.5em;
    height: 18px;
}

#sortable li span {
    position: absolute;
    margin-left: 1.3em;
}

.modal-body p {
  font-family: "Lucida Grande", "Lucida Sans Unicode", Arial, sans-serif;
  font-size: 1.8em;
  margin: auto;
}

.modal-body {
  max-width: 100%;
}

.canvasjs-chart-canvas {
  max-width: 100%;
  max-height: 420px;
}

